{
  "name": "collaborative-todo-app",
  "version": "1.0.0",
  "main": "electron.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --dev",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"npm run start\" \"wait-on http://localhost:5055 && electron .\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-terser": "^0.4.4",
    "concurrently": "^6.2.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.6.0"
  }
}
